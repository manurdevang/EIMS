class Menu::MenuGroupsMenuLinksController < ApplicationController
	
	def index
	@menugroupsmenulinks=MenuGroup.all
	end

	def new
		@menugroupsmenulink=MenuGroupMenuLink.new
	end	
	
	def create
		@menugroupsmenulink=MenuGroupMenuLink.new()
	end
	
	def edit
		@menugroup=MenuGroupMenuLink.find(params[:id])
		respond_to do |format|
			format.js
			format.html
		end
  end
	
